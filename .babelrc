/* 
    ./.babelrc
*/  
{
  "presets": [
    "es2015",
    "stage-0",
    "babel-preset-stage-0",
    "react"

  ],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-async-to-generator",

    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ]
  ]
}